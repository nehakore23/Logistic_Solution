<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shippingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Starpage"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewRegistration"></activity>
        <activity android:name="AdminDashboard"></activity>
        <activity android:name=".UserDashboard"></activity>
        <activity android:name=".DriverDashboard"></activity>
        <activity android:name=".ScheduldShip"></activity>
        <activity android:name=".Company"></activity>
        <activity android:name=".ViewShipment"></activity>
        <activity android:name=".EditProfile"></activity>
        <activity android:name=".ViewAvailableShipment"></activity>
        <activity android:name=".ViewAcceptedShipment"></activity>
        <activity android:name=".EditDriverProfile"></activity>
        <activity android:name=".AddStock"></activity>
        <activity android:name=".WareHouseDashboard"></activity>
        <activity android:name=".ViewStock"></activity>
        <activity android:name=".ConfirmUser"></activity>
        <activity android:name=".ViewConfirmUser"></activity>
        <activity android:name=".Login"></activity>
    </application>

</manifest>